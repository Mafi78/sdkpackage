
<component
	id="FallbackPicture"
	title="Fallback Picture 2.0"
	tooltip="Fallback Picture"
	icon="res/FallbackPicture/FallbackPicture.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/FallbackPicture/FallbackPicture</requireJs>

	<cssInclude>res/FallbackPicture/FallbackPicture.css</cssInclude>
	
	
	<property
		id="fallbackPicture"
		type="Url"
		title="Url For Fallback Picture"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="picture"
		type="Url"
		title="Url For Picture"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">100</defaultValue>
		<defaultValue property="HEIGHT">100</defaultValue>
		
		<defaultValue property="fallbackPicture"><![CDATA[]]></defaultValue>
		<defaultValue property="picture"><![CDATA[]]></defaultValue>

	</initialization>

</component>
