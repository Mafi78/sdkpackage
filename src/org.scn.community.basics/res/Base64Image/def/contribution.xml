
<component id="Base64Image"
	title="Base64 Image Encoder 2.0"
	icon="res/Base64Image/Base64Image.png"
	tooltip="Handy Base-64 encoder property sheet (requires IE10+ for design-time)"
	handlerType="sapui5"
	modes="commons m"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics">
	
	<requireJs modes="commons">res/Base64Image/Base64Image_commons</requireJs>
	<requireJs modes="m">res/Base64Image/Base64Image</requireJs>
	
	<property type="String" title="Image Source URI" id="src" group="Display"/>
	<property type="ScriptText" title="On Click" group="Events" id="onclick"></property>			
	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="HEIGHT">auto</defaultValue>
		<defaultValue property="WIDTH">auto</defaultValue>
		<defaultValue property="src">data:image/png;base64,</defaultValue>
	</initialization>
</component>