
<component
	id="TriStateCheckBox"
	title="Tri State CheckBox 2.0"
	tooltip="Tri State CheckBox"
	icon="res/TriStateCheckBox/TriStateCheckBox.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/TriStateCheckBox/TriStateCheckBox</requireJs>

	<cssInclude>res/TriStateCheckBox/TriStateCheckBox.css</cssInclude>
	
	
	<property
		id="editable"
		type="boolean"
		title="Editable"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="enabled"
		type="boolean"
		title="Enabled"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onSelectionChanged"
		type="ScriptText"
		title="Event For On Selection Changed"
		group="Events"
		visible="true">

	</property>
	<property
		id="selectionState"
		type="String"
		title="Tooltip"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="text"
		type="String"
		title="Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="tooltip"
		type="String"
		title="Tooltip"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">22</defaultValue>
		
		<defaultValue property="editable"><![CDATA[true]]></defaultValue>
		<defaultValue property="enabled"><![CDATA[true]]></defaultValue>
		<defaultValue property="onSelectionChanged"><![CDATA[]]></defaultValue>
		<defaultValue property="selectionState"><![CDATA[Unchecked]]></defaultValue>
		<defaultValue property="text"><![CDATA[tri state value]]></defaultValue>
		<defaultValue property="tooltip"><![CDATA[]]></defaultValue>

	</initialization>

</component>
