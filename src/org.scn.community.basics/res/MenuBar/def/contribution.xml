
<component
	id="MenuBar"
	title="Menu Bar 2.0"
	tooltip="Menu Bar"
	icon="res/MenuBar/MenuBar.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/MenuBar/MenuBar</requireJs>

	<cssInclude>res/MenuBar/MenuBar.css</cssInclude>
	
	
	<property
		id="cleanAll"
		type="boolean"
		title="Clean All Buttons"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="defaultImage"
		type="Url"
		title="Url For Default Image"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="elementsContent"
		type="String"
		title="Buttons"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onSelectionChanged"
		type="ScriptText"
		title="Event For On Selection Changed"
		group="Events"
		visible="true">

	</property>
	<property
		id="selectedKey"
		type="String"
		title="Technical Proprty For Selected Key"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="selectedText"
		type="String"
		title="Technical Proprty For Selected Text"
		group="Display"
		bindable="true"
		visible="false">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">400</defaultValue>
		<defaultValue property="HEIGHT">29</defaultValue>
		
		<defaultValue property="cleanAll"><![CDATA[false]]></defaultValue>
		<defaultValue property="defaultImage"><![CDATA[]]></defaultValue>
		<defaultValue property="elementsContent"><![CDATA[[]]]></defaultValue>
		<defaultValue property="onSelectionChanged"><![CDATA[]]></defaultValue>
		<defaultValue property="selectedKey"><![CDATA[]]></defaultValue>
		<defaultValue property="selectedText"><![CDATA[]]></defaultValue>

	</initialization>

</component>
