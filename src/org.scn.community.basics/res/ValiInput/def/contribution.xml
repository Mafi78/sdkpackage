
<component
	id="ValiInput"
	title="Validated Input Field 2.0"
	tooltip="Validated Input Field"
	icon="res/ValiInput/ValiInput.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/ValiInput/ValiInput</requireJs>

	<cssInclude>res/ValiInput/ValiInput.css</cssInclude>
	
	
	<property
		id="DDefault"
		type="String"
		title="Default Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLength"
		type="int"
		title="Length"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DNumber"
		type="float"
		title="Number"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DTooltip"
		type="String"
		title="Tooltip"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValidState"
		type="String"
		title="Valid State (for Custom)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValidation"
		type="String"
		title="Validation Type"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValue"
		type="String"
		title="Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onChanged"
		type="ScriptText"
		title="Event For On Changed"
		group="Events"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">22</defaultValue>
		
		<defaultValue property="DDefault"><![CDATA[]]></defaultValue>
		<defaultValue property="DLength"><![CDATA[10]]></defaultValue>
		<defaultValue property="DNumber"><![CDATA[10]]></defaultValue>
		<defaultValue property="DTooltip"><![CDATA[]]></defaultValue>
		<defaultValue property="DValidState"><![CDATA[None]]></defaultValue>
		<defaultValue property="DValidation"><![CDATA[none]]></defaultValue>
		<defaultValue property="DValue"><![CDATA[]]></defaultValue>
		<defaultValue property="onChanged"><![CDATA[]]></defaultValue>

	</initialization>

</component>
