
<component
	id="ProgressIndicator"
	title="Progress Indicator 2.0"
	tooltip="Progress Indicator"
	icon="res/ProgressIndicator/ProgressIndicator.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/ProgressIndicator/ProgressIndicator</requireJs>

	<cssInclude>res/ProgressIndicator/ProgressIndicator.css</cssInclude>
	
	
	<property
		id="barColor"
		type="String"
		title="Bar Colour"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="displayValue"
		type="String"
		title="Display Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="enabled"
		type="boolean"
		title="Enabled"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="percentValue"
		type="int"
		title="Percent Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="showValue"
		type="boolean"
		title="Show Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">270</defaultValue>
		<defaultValue property="HEIGHT">20</defaultValue>
		
		<defaultValue property="barColor"><![CDATA[Neutral]]></defaultValue>
		<defaultValue property="displayValue"><![CDATA[]]></defaultValue>
		<defaultValue property="enabled"><![CDATA[true]]></defaultValue>
		<defaultValue property="percentValue"><![CDATA[50]]></defaultValue>
		<defaultValue property="showValue"><![CDATA[true]]></defaultValue>

	</initialization>

</component>
