
<component id="FioriSlider" 
	title="Fiori Slider"
	tooltip="sap.m Slider"
	icon="res/FioriSlider/FioriSlider.png"
	handlerType="sapui5" 
	modes="m"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics">

	<requireJs modes="m">res/FioriSlider/FioriSlider</requireJs>
  	<property id="enabled" type="boolean" title="Enabled"/>
	<property id="min" type="float" title="Minimum"/>
	<property id="max" type="float" title="Maximum"/>
  	<property id="step" type="float" title="Step"/>
  	<property id="value" type="float" title="Value"/>
  	<property id="progress" type="boolean" title="Progress"/>
  	<property id="showHandleTooltip" type="boolean" title="Show Handle Tooltip"/>
  	<property id="onchange" type="ScriptText" title="On Change" group="Events"/>
 	<initialization>
 		<defaultValue property="value">0</defaultValue>
 		<defaultValue property="min">0</defaultValue>
 		<defaultValue property="max">100</defaultValue>
 		<defaultValue property="step">1</defaultValue>
 		<defaultValue property="progress">true</defaultValue>
 		<defaultValue property="showHandleTooltip">true</defaultValue>
 		<defaultValue property="enabled">true</defaultValue>
 	</initialization>
 </component>