
<component
	id="LeaderBoard"
	title="Leader Board 2.0"
	tooltip="Leader Board"
	icon="res/LeaderBoard/LeaderBoard.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityVisualizations"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/LeaderBoard/LeaderBoard</requireJs>

	<cssInclude>res/LeaderBoard/LeaderBoard.css</cssInclude>
	
	
	<property
		id="addCounter"
		type="boolean"
		title="Add Counter"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="cleanAll"
		type="boolean"
		title="Clean All Elements"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="elements"
		type="String"
		title="Elements"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="fallbackPicture"
		type="Url"
		title="Fallback Picture Url"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onPress"
		type="ScriptText"
		title="Event For On Press"
		group="Events"
		visible="true">

	</property>
	<property
		id="onSelectionChanged"
		type="ScriptText"
		title="Event For On Selection Changed"
		group="Events"
		visible="true">

	</property>
	<property
		id="pressedKey"
		type="String"
		title="Technical Proprty For Pressed Key"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="selectedKey"
		type="String"
		title="Technical Proprty For Selected Key"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="valuePrefix"
		type="String"
		title="Value Prefix Text"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="valueSuffix"
		type="String"
		title="Value Suffix Text"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">230</defaultValue>
		<defaultValue property="HEIGHT">400</defaultValue>
		
		<defaultValue property="addCounter"><![CDATA[true]]></defaultValue>
		<defaultValue property="cleanAll"><![CDATA[false]]></defaultValue>
		<defaultValue property="elements"><![CDATA[[]]]></defaultValue>
		<defaultValue property="fallbackPicture"><![CDATA[]]></defaultValue>
		<defaultValue property="onPress"><![CDATA[]]></defaultValue>
		<defaultValue property="onSelectionChanged"><![CDATA[]]></defaultValue>
		<defaultValue property="pressedKey"><![CDATA[]]></defaultValue>
		<defaultValue property="selectedKey"><![CDATA[]]></defaultValue>
		<defaultValue property="valuePrefix"><![CDATA[]]></defaultValue>
		<defaultValue property="valueSuffix"><![CDATA[]]></defaultValue>

	</initialization>

</component>
