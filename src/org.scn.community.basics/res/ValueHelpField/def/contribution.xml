
<component
	id="ValueHelpField"
	title="Value Help Field 2.0"
	tooltip="Value Help Field"
	icon="res/ValueHelpField/ValueHelpField.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/ValueHelpField/ValueHelpField</requireJs>

	<cssInclude>res/ValueHelpField/ValueHelpField.css</cssInclude>
	
	
	<property
		id="editable"
		type="boolean"
		title="Editable"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="enabled"
		type="boolean"
		title="Enabled"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onChanged"
		type="ScriptText"
		title="Event For On Changed"
		group="Events"
		visible="true">

	</property>
	<property
		id="onValueHelpRequest"
		type="ScriptText"
		title="Event For On Value Help Request"
		group="Events"
		visible="true">

	</property>
	<property
		id="tooltip"
		type="String"
		title="Tooltip"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="value"
		type="String"
		title="Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">22</defaultValue>
		
		<defaultValue property="editable"><![CDATA[true]]></defaultValue>
		<defaultValue property="enabled"><![CDATA[true]]></defaultValue>
		<defaultValue property="onChanged"><![CDATA[]]></defaultValue>
		<defaultValue property="onValueHelpRequest"><![CDATA[]]></defaultValue>
		<defaultValue property="tooltip"><![CDATA[]]></defaultValue>
		<defaultValue property="value"><![CDATA[some value]]></defaultValue>

	</initialization>

</component>
