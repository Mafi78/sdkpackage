
<component
	id="JsonObject"
	title="Json Object"
	tooltip="Utility for work with Json Object"
	icon="res/JsonObject/JsonObject.png"
	handlerType="div"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityUtils"
	databound="false"
	modes="commons"
>
	
	<requireJs modes="commons">res/JsonObject/JsonObject</requireJs>

	<cssInclude>res/JsonObject/JsonObject.css</cssInclude>
	
	
	<property
		id="content"
		type="String"
		title="Properties"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">10</defaultValue>
		<defaultValue property="HEIGHT">10</defaultValue>
		
		<defaultValue property="content"><![CDATA[{}]]></defaultValue>

	</initialization>

</component>
