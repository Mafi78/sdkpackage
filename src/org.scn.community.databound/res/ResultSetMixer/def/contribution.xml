
<component
	id="ResultSetMixer"
	title="Result Set Mixer 2.0"
	tooltip="Result Set Mixer"
	icon="res/ResultSetMixer/ResultSetMixer.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="true"
	modes="commons"
>
	
	<requireJs modes="commons">res/ResultSetMixer/ResultSetMixer</requireJs>

	<cssInclude>res/ResultSetMixer/ResultSetMixer.css</cssInclude>
	
	
	<property
		id="DCollectMultipleMatches"
		type="String"
		title="Collect Multiple Matches"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DMasterGeometry"
		type="String"
		title="Geometry Of The Master Result Set"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DMasterProvisioner"
		type="String"
		title="Master Central Data Provisioner"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DReloadTrigger"
		type="float"
		title="Internal Property To Trigger The Reload"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="DSlaveColumnCondition"
		type="String"
		title="Column Condition To Read Slave Result Set"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSlaveColumnIndex"
		type="int"
		title="Column Index For The Selection From Slave Result Set"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSlaveContentCondition"
		type="String"
		title="Content Condition To Read Slave Result Set"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSlaveProvisioner"
		type="String"
		title="Slave Central Data Provisioner"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSlaveRowCondition"
		type="String"
		title="Row Condition To Read Slave Result Set"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">600</defaultValue>
		<defaultValue property="HEIGHT">400</defaultValue>
		
		<defaultValue property="DCollectMultipleMatches"><![CDATA[Collect]]></defaultValue>
		<defaultValue property="DMasterGeometry"><![CDATA[Rows]]></defaultValue>
		<defaultValue property="DMasterProvisioner"><![CDATA[]]></defaultValue>
		<defaultValue property="DReloadTrigger"><![CDATA[0.0]]></defaultValue>
		<defaultValue property="DSlaveColumnCondition"><![CDATA[]]></defaultValue>
		<defaultValue property="DSlaveColumnIndex"><![CDATA[1]]></defaultValue>
		<defaultValue property="DSlaveContentCondition"><![CDATA[]]></defaultValue>
		<defaultValue property="DSlaveProvisioner"><![CDATA[]]></defaultValue>
		<defaultValue property="DSlaveRowCondition"><![CDATA[]]></defaultValue>

	</initialization>

</component>
