
<component
	id="FioriLink"
	title="Fiori Link 2.0"
	tooltip="Fiori Link - Component for links."
	icon="res/FioriLink/FioriLink.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="m"
>
	
	<requireJs modes="m">res/FioriLink/FioriLink</requireJs>

	<cssInclude>res/FioriLink/FioriLink.css</cssInclude>
	
	
	<property
		id="contentWidth"
		type="int"
		title="Content Width"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="emphasized"
		type="boolean"
		title="Emphasized"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="enabled"
		type="boolean"
		title="Enabled"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="href"
		type="Url"
		title="Href"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onPressed"
		type="ScriptText"
		title="On Pressed"
		group="Events"
		visible="true">

	</property>
	<property
		id="pressedKey"
		type="String"
		title="Pressed Key"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="subtle"
		type="boolean"
		title="Subtle"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="target"
		type="String"
		title="Target"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="text"
		type="String"
		title="Text"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="textAlign"
		type="String"
		title="Text Align"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="textDirection"
		type="String"
		title="Text Direction"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="useContentWidth"
		type="boolean"
		title="Use Manual 'Content Width'"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="useHref"
		type="boolean"
		title="Use Manual 'Href'"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="wrapping"
		type="boolean"
		title="Wrapping"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">420</defaultValue>
		<defaultValue property="HEIGHT">580</defaultValue>
		
		<defaultValue property="contentWidth"><![CDATA[0]]></defaultValue>
		<defaultValue property="emphasized"><![CDATA[false]]></defaultValue>
		<defaultValue property="enabled"><![CDATA[true]]></defaultValue>
		<defaultValue property="href"><![CDATA[]]></defaultValue>
		<defaultValue property="onPressed"><![CDATA[]]></defaultValue>
		<defaultValue property="pressedKey"><![CDATA[]]></defaultValue>
		<defaultValue property="subtle"><![CDATA[false]]></defaultValue>
		<defaultValue property="target"><![CDATA[]]></defaultValue>
		<defaultValue property="text"><![CDATA[]]></defaultValue>
		<defaultValue property="textAlign"><![CDATA[Initial]]></defaultValue>
		<defaultValue property="textDirection"><![CDATA[Inherit]]></defaultValue>
		<defaultValue property="useContentWidth"><![CDATA[true]]></defaultValue>
		<defaultValue property="useHref"><![CDATA[true]]></defaultValue>
		<defaultValue property="wrapping"><![CDATA[false]]></defaultValue>

	</initialization>

</component>
