
<component
	id="Table"
	title="Data Table 2.0"
	tooltip="Data Table"
	icon="res/Table/Table.png"
	handlerType="div"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityVisualizations"
	databound="true"
	modes="commons"
>
	
	<requireJs modes="commons">res/Table/Table</requireJs>

	<cssInclude>res/Table/Table.css</cssInclude>
	
	
	<property
		id="column1"
		type="ResultCellList"
		title="Column 1"
		group="DataBinding"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="column2"
		type="ResultCellList"
		title="Column 2"
		group="DataBinding"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="column3"
		type="ResultCellList"
		title="Column 3"
		group="DataBinding"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="data"
		type="ResultCellList"
		title="Column 1"
		group="DataBinding"
		visible="false"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">400</defaultValue>
		<defaultValue property="HEIGHT">400</defaultValue>
		
		<defaultValue property="column1"><![CDATA[]]></defaultValue>
		<defaultValue property="column2"><![CDATA[]]></defaultValue>
		<defaultValue property="column3"><![CDATA[]]></defaultValue>
		<defaultValue property="data"><![CDATA[]]></defaultValue>

	</initialization>

</component>
