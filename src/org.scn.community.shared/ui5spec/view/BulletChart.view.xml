<core:View
  xmlns="sap.package"
  xmlns:core="sap.ui.core"
  xmlns:l="sap.ui.layout"
  xmlns:m="sap.m"
  
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  
  controllerName="org.scn.community.%COMP-package%.%COMP-id%.Controller"
  >
  
  <l:VerticalLayout
    width="100%" content="{/}">
    <l:content
		width="{width}"
	>
		<BulletChart
				actual="{actual}"
				actualValueLabel="{actualValueLabel}"
				contentWidth="{contentWidth}"
				deltaValueLabel="{deltaValueLabel}"
				forecastValue="{forecastValue}"
				maxValue="{maxValue}"
				minValue="{minValue}"
				mode="{mode}"
				onPressed="{onPressed}"
				pressedKey="{pressedKey}"
				scale="{scale}"
				scaleColor="{scaleColor}"
				showActualValue="{showActualValue}"
				showDeltaValue="{showDeltaValue}"
				showTargetValue="{showTargetValue}"
				showValueMarker="{showValueMarker}"
				size="{size}"
				targetValue="{targetValue}"
				targetValueLabel="{targetValueLabel}"
		
		>
	     	
			
		
		
		
		
		
		
		
		<onPressed>
			
		</onPressed>
		
		
		
		
		
		
		
		
		
		
		

		</BulletChart>
	</l:content>
  </l:VerticalLayout> 
</core:View>