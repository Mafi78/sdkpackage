<core:View
  xmlns="sap.package"
  xmlns:core="sap.ui.core"
  xmlns:l="sap.ui.layout"
  xmlns:m="sap.m"
  
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  
  controllerName="org.scn.community.%COMP-package%.%COMP-id%.Controller"
  >
  
  <l:VerticalLayout
    width="100%" content="{/}">
    <l:content
		width="{width}"
	>
		<MicroAreaChart
				chart="{chart}"
				contentHeight="{contentHeight}"
				contentWidth="{contentWidth}"
				maxXValue="{maxXValue}"
				maxYValue="{maxYValue}"
				minXValue="{minXValue}"
				minYValue="{minYValue}"
				onPressed="{onPressed}"
				pressedKey="{pressedKey}"
				view="{view}"
		
		>
	     	
			
		
		
		
		
		
		
		<onPressed>
			
		</onPressed>
		
		
		

		</MicroAreaChart>
	</l:content>
  </l:VerticalLayout> 
</core:View>