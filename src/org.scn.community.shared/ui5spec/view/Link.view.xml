<core:View
  xmlns="sap.package"
  xmlns:core="sap.ui.core"
  xmlns:l="sap.ui.layout"
  xmlns:m="sap.m"
  
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  
  controllerName="org.scn.community.%COMP-package%.%COMP-id%.Controller"
  >
  
  <l:VerticalLayout
    width="100%" content="{/}">
    <l:content
		width="{width}"
	>
		<Link
				contentWidth="{contentWidth}"
				emphasized="{emphasized}"
				enabled="{enabled}"
				href="{href}"
				onPressed="{onPressed}"
				pressedKey="{pressedKey}"
				subtle="{subtle}"
				target="{target}"
				text="{text}"
				textAlign="{textAlign}"
				textDirection="{textDirection}"
				wrapping="{wrapping}"
		
		>
	     	
			
		
		
		
		<onPressed>
			
		</onPressed>
		
		
		
		
		
		
		
		

		</Link>
	</l:content>
  </l:VerticalLayout> 
</core:View>