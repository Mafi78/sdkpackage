<core:View
  xmlns="sap.package"
  xmlns:core="sap.ui.core"
  xmlns:l="sap.ui.layout"
  xmlns:m="sap.m"
  
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  
  controllerName="org.scn.community.%COMP-package%.%COMP-id%.Controller"
  >
  
  <l:VerticalLayout
    width="100%" content="{/}">
    <l:content
		width="{width}"
	>
		<RadioButton
				activeHandling="{activeHandling}"
				contentWidth="{contentWidth}"
				editable="{editable}"
				enabled="{enabled}"
				groupName="{groupName}"
				onSelected="{onSelected}"
				selected="{selected}"
				selectedKey="{selectedKey}"
				text="{text}"
				textAlign="{textAlign}"
				textDirection="{textDirection}"
				valueState="{valueState}"
		
		>
	     	
			
		
		
		
		
		<onSelected>
			
		</onSelected>
		
		
		
		
		
		
		

		</RadioButton>
	</l:content>
  </l:VerticalLayout> 
</core:View>